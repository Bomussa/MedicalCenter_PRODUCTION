{
  "name": "attarmedical_backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development node server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:client": "cd client && npm ci && npm run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx || true",
    "format": "prettier -w . || true",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "reports:run": "node jobs/reportScheduler.js --run-once",
    "qa:static": "node scripts/staticCheck.js",
    "start:api": "node server.js",
    "start:frontend": "cd frontend && npm run dev",
    "start:all": "npm run start:api & (cd frontend && npm run dev)"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "yamljs": "^0.3.0",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "node-cron": "^3.0.3",
    "json2csv": "^6.0.0-alpha.2",
    "pdfkit": "^0.15.0"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "eslint": "^9.8.0",
    "prettier": "^3.3.3"
  }
}